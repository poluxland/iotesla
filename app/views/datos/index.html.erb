<h1>Datos</h1>




<%= line_chart [
  {name: 'Sensor 1 Temp', data: Dato.pluck(:created_at, :sensor1)},
  {name: 'Sensor 2 Temp', data: Dato.pluck(:created_at, :sensor2)},
  {name: 'Sensor 3 Temp', data: Dato.pluck(:created_at, :sensor3)},
  {name: 'Sensor 4 Temp', data: Dato.pluck(:created_at, :sensor4)},
  {name: 'Sensor 5 Temp', data: Dato.pluck(:created_at, :sensor5)},
  {name: 'Sensor 6 Temp', data: Dato.pluck(:created_at, :sensor6)},
  {name: 'Sensor 7 Humedad', data: Dato.pluck(:created_at, :sensor7)},
  {name: 'Sensor 8 Corriente', data: Dato.pluck(:created_at, :sensor8)},
  {name: 'Sensor 9 Corriente', data: Dato.pluck(:created_at, :sensor9)},
  {name: 'Sensor 10 Corriente', data: Dato.pluck(:created_at, :sensor10)},
  {name: 'Sensor 11 psi', data: Dato.pluck(:created_at, :sensor11)},
  {name: 'Sensor 12 psi', data: Dato.pluck(:created_at, :sensor12)},
  {name: 'Sensor 13 m/s', data: Dato.pluck(:created_at, :sensor13)},
  {name: 'Sensor 14 alarma', data: Dato.pluck(:created_at, :sensor14)},
  {name: 'Sensor 15 alarma', data: Dato.pluck(:created_at, :sensor15)},
  {name: 'Sensor 16 alarma', data: Dato.pluck(:created_at, :sensor16)},
  {name: 'Sensor 17 alarma', data: Dato.pluck(:created_at, :sensor17)},
  {name: 'Sensor 18 alarma', data: Dato.pluck(:created_at, :sensor18)}
] %>



<br>
<br>

<table id="myTable" class="table table-striped table-bordered shadow table-bordered m-3" style="width: 1200px;">

  <thead>
    <tr>
      <th>Creado</th>
      <th>Modulo</th>
      <th>Sensor 1 Temp °C</th>
      <th>Sensor 2 Temp °C</th>
      <th>Sensor 3 Temp °C</th>
      <th>Sensor 4 Temp °C</th>
      <th>Sensor 5 Temp °C</th>
      <th>Sensor 6 Temp °C</th>
      <th>Sensor 7 Humedad %</th>
      <th>Sensor 8 Corriente A</th>
      <th>Sensor 9 Corriente A</th>
      <th>Sensor 10 Corriente A</th>
      <th>Sensor 11 Presión psi</th>
      <th>Sensor 12 Presión psi</th>
      <th>Sensor 13 Vibración m/s</th>
      <th>Sensor 14 Alarma</th>
      <th>Sensor 15 Alarma</th>
      <th>Sensor 16 Alarma</th>
      <th>Sensor 17 Alarma</th>
      <th>Sensor 18 Alarma</th>
      <th>Acciones</th>
    </tr>
  </thead>
  <tbody>
    <% @datos.each do |dato| %>
      <tr id="<%= dom_id dato %>">
        <td><%= dato.created_at %></td>
        <td><%= dato.modulo %></td>
        <td><%= dato.sensor1 %></td>
        <td><%= dato.sensor2 %></td>
        <td><%= dato.sensor3 %></td>
        <td><%= dato.sensor4 %></td>
        <td><%= dato.sensor5 %></td>
        <td><%= dato.sensor6 %></td>
        <td><%= dato.sensor7 %></td>
        <td><%= dato.sensor8 %></td>
        <td><%= dato.sensor9 %></td>
        <td><%= dato.sensor10 %></td>
        <td><%= dato.sensor11 %></td>
        <td><%= dato.sensor12 %></td>
        <td><%= dato.sensor13 %></td>
        <td><%= dato.sensor14 %></td>
        <td><%= dato.sensor15 %></td>
        <td><%= dato.sensor16 %></td>
        <td><%= dato.sensor17 %></td>
        <td><%= dato.sensor18 %></td>
        <td>
          <%= link_to "Ver", dato, class: "btn btn-primary" %>
          <%= link_to "Editar", edit_dato_path(dato), class: "btn btn-secondary" %>
          <%= link_to "Borrar", dato, method: :delete, data: { confirm: "Esta seguro?" }, remote: true, class: "btn btn-danger" %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>


<%= link_to "Agregar dato", new_dato_path,  class: "btn btn-primary" %>



